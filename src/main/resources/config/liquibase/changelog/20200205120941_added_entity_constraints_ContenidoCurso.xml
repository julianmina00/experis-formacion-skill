<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ContenidoCurso.
    -->
    <changeSet id="20200205120941-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="curso_id"
                                 baseTableName="contenido_curso"
                                 constraintName="fk_contenido_curso_curso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="curso"/>

        <addForeignKeyConstraint baseColumnNames="interes_id"
                                 baseTableName="contenido_curso"
                                 constraintName="fk_contenido_curso_interes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="interes"/>

        <addForeignKeyConstraint baseColumnNames="habilidad_id"
                                 baseTableName="contenido_curso"
                                 constraintName="fk_contenido_curso_habilidad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="habilidad"/>

    </changeSet>
</databaseChangeLog>
